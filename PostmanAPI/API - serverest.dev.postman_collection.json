	"item": [
		{
			"name": "Cadastrar usuário",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 201) {\r",
							"    pm.test(\"Status code is 201\", function () {\r",
							"        pm.response.to.have.status(201);\r",
							"    });\r",
							"\r",
							"    pm.test(\"Mensagem de sucesso\", function () {\r",
							"        var jsonData = pm.response.json();\r",
							"        pm.expect(jsonData.message).to.eql(\"Cadastro realizado com sucesso\");\r",
							"    });\r",
							"} else if (pm.response.code === 400) {\r",
							"    pm.test(\"Status code is 400\", function () {\r",
							"        pm.response.to.have.status(400);\r",
							"    });\r",
							"\r",
							"    pm.test(\"Mensagem de erro\", function () {\r",
							"        var jsonData = pm.response.json();\r",
							"        pm.expect(jsonData.message).to.include(\"Este email já está sendo usado\");\r",
							"    });\r",
							"}\r",
							"\r",
							"var jsonData = pm.request.body ? JSON.parse(pm.request.body.raw) : {};\r",
							"pm.collectionVariables.set(\"email\", jsonData.email);\r",
							"\r",
							"pm.test(\"Validação da mensagem de resposta\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"\r",
							"    if (jsonData.message === \"Cadastro realizado com sucesso\") {\r",
							"        pm.expect(jsonData.message).to.eql(\"Cadastro realizado com sucesso\");\r",
							"    } else if (jsonData.message === \"Este email já está sendo usado\") {\r",
							"        pm.expect(jsonData.message).to.eql(\"Este email já está sendo usado\");\r",
							"    } else {\r",
							"        pm.expect.fail(\"Mensagem inesperada: \" + jsonData.message);\r",
							"    }\r",
							"});\r",
							"\r",
							"// Parse the response JSON\r",
							"const responseJson = pm.response.json();\r",
							"\r",
							"// Save the _id to a collection variable\r",
							"pm.collectionVariables.set(\"user_id\", responseJson._id);\r",
							"\r",
							"// Optional: Log to console to verify it worked\r",
							"console.log(\"Saved _id:\", responseJson._id);\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
